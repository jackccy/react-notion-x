{
  "name": "@jackrepo/notion-client",
  "version": "6.16.7",
  "private": false,
  "type": "module",
  "description": "Robust TypeScript client for the unofficial Notion API.",
  "repository": "@jackrepo/react-notion-x",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "sideEffects": false,
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "test": "ava"
  },
  "dependencies": {
    "got": "^11.8.1",
    "notion-types": "^6.16.3",
    "notion-utils": "^6.16.3",
    "p-map": "^5.3.0"
  },
  "ava": {
    "snapshotDir": ".snapshots",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ]
  },
  "gitHead": "08ae19994cdfb708dc179731afa24a622045a66b"
}
