{
  "name": "@jackrepo/notion-client",
  "version": "6.16.11",
  "type": "module",
  "description": "Robust TypeScript client for the unofficial Notion API.",
  "repository": "@jackrepo/react-notion-x",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "test": "ava"
  },
  "dependencies": {
    "got": "^11.8.1",
    "notion-types": "^6.16.3",
    "notion-utils": "^6.16.3",
    "p-map": "^5.3.0"
  },
  "ava": {
    "snapshotDir": ".snapshots",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ]
  },
  "gitHead": "57fe4e9e92608e6d0c32cb268fc5fa99fcee709a"
}
